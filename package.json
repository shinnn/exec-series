{
  "name": "exec-series",
  "version": "1.0.1",
  "description": "Run commands in order",
  "repository": "shinnn/exec-series",
  "author": {
    "name": "Shinnosuke Watanabe",
    "url": "https://github.com/shinnn"
  },
  "scripts": {
    "pretest": "jscs *.js && eslint *.js",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/exec-series/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js",
    "LICENSE"
  ],
  "keywords": [
    "child_process",
    "exec",
    "series",
    "serial",
    "serially",
    "command",
    "sequence",
    "sequential",
    "sequentially",
    "order"
  ],
  "dependencies": {
    "async-each-series": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^0.10.2",
    "is-appveyor": "^1.0.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.8.1",
    "rimraf": "^2.2.8",
    "tap-spec": "^2.1.2",
    "tape": "^3.0.3"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "validateLineBreaks": "LF"
  }
